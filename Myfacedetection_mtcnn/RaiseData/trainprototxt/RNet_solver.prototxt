net: "RNet_train.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.001  #0.01
momentum: 0.9
weight_decay: 0.004
# The learning rate policy
lr_policy: "step"
stepsize: 10000 #40000
gamma: 0.8
display: 100
max_iter: 50000
snapshot: 5000 #100000
snapshot_prefix: "./model/"
solver_mode: GPU
